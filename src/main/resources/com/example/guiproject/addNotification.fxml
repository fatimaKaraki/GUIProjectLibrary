<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="287.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.guiproject.AddNotification">
   <children>
      <Button fx:id="sendNotification" layoutX="35.0" layoutY="317.0" mnemonicParsing="false" onAction="#sendNotification" text="Send Notifications" />
      <Button fx:id="clearMsg" layoutX="162.0" layoutY="317.0" mnemonicParsing="false" onAction="#clear" text="Clear" />
      <TextArea fx:id="msgBody" layoutX="30.0" layoutY="100.0" prefHeight="200.0" prefWidth="200.0" />
      <TextField fx:id="msgTitle" layoutX="35.0" layoutY="51.0" />
      <Label layoutX="38.0" layoutY="24.0" text="Message Title:">
         <font>
            <Font size="17.0" />
         </font></Label>
      <Label layoutX="35.0" layoutY="77.0" text="Message Body:">
         <font>
            <Font size="17.0" />
         </font></Label>
      <Label fx:id="msgResult" layoutX="130.0" layoutY="368.0" text="Label" />
   </children>
</AnchorPane>
